# EmailJS Configuration Example
# Copy this file to .env and replace with your actual EmailJS credentials

# Your EmailJS Service ID (from Email Services section)
VITE_EMAILJS_SERVICE_ID=service_abc123

# Your EmailJS Template ID (from Email Templates section)  
VITE_EMAILJS_TEMPLATE_ID=template_xyz789

# Your EmailJS Public Key (from Account > General section)
VITE_EMAILJS_PUBLIC_KEY=user_def456

# Optional: Custom sender name
VITE_EMAILJS_FROM_NAME=EVOL Jewels